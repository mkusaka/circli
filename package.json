{
  "name": "circli",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/mkusaka/circli.git",
  "author": "mkusaka <hinoshita1992@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.30",
    "@types/prettier": "^2.6.0",
    "@vercel/ncc": "^0.33.4",
    "fast-glob": "^3.2.11",
    "ncc": "^0.3.6",
    "openapi-typescript-codegen": "^0.22.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "update": "curl -o swagger.json https://circleci.com/api/v2/openapi.json && prettier --write './**/*.{json,ts}' && yarn gen && yarn transform",
    "gen": "openapi -i swagger.json -o client --useOptions --useUnionTypes --indent 2",
    "run": "deno run -A main.ts",
    "transform": "ncc run transform.ts",
    "fmt": "prettier --write transform.ts"
  }
}
